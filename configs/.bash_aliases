# Default bash aliases for entropy_bootstrap_linux
# This file is intended to be copied to the user's home directory by setup.sh

# entropy_bootstrap_linux aliases - start
alias update='sudo apt update && sudo apt upgrade -y'
alias ll='ls -lh --color=auto'
alias cls='clear'
alias edit='nano'
alias rebootnow='sudo reboot'
# entropy_bootstrap_linux aliases - end
